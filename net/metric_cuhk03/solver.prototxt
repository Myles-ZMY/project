net: "train_val.prototxt"

# TRAINing rate and policy
lr_policy: "inv"
base_lr: 0.1
gamma: 0.0001
power: 0.75
stepsize: 10000

# TRAINing iter
max_iter: 6000

# TESTing iter and interval
test_iter: 1
test_interval: 200

# display
display: 20

# snapshots
snapshot: 2000
snapshot_prefix: "snapshots/train_val"

# GPU / CPU
solver_mode: GPU
