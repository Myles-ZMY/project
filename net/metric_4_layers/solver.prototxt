net: "train_val.prototxt"

# TRAINing rate and policy
lr_policy: "step"
base_lr: 0.01
gamma: 0.1
stepsize: 20000
momentum: 0.9
weight_decay: 0.0005

# TRAINing iter
max_iter: 20000

# TESTing iter and interval
test_iter: 1
test_interval: 200

# display
display: 20

# snapshots
snapshot: 5000
snapshot_prefix: "snapshots/train_val"

# GPU / CPU
solver_mode: GPU
